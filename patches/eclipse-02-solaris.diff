+	int err = statfs(lpDirectoryName,&buffer,sizeof(struct statfs),0);
+	if (err==0){
+		long size = buffer.f_bsize;
+		jlong free = buffer.f_bfree;
+		result = size*free;
+	}
+
+	return result;
