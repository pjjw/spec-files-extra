-    {
-        x = parent->State.Width + x ;
-        if ( w >= 0 ) x -= w ;
-    }
-
-    if ( w < 0 ) w = parent->State.Width - x + w ;
-    if ( w < 0 )
-    {
-        x += w ;
-        w = -w ;
-    }
-
-    if ( y < 0 )
-    {
-        y = parent->State.Height + y ;
-        if ( h >= 0 ) y -= h ;
-    }
-
-    if ( h < 0 ) h = parent->State.Height - y + h ;
-    if ( h < 0 )
-    {
-        y += h ;
-        h = -h ;
-    }
-
-    window = fgCreateWindow( parent, "", GL_TRUE, x, y, GL_TRUE, w, h, GL_FALSE, GL_FALSE );
