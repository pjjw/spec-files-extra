+static void fghcbCheckJoystickPolls( SFG_Window *window,
+                                     SFG_Enumerator *enumerator )
+{
+    long int checkTime = fgElapsedTime( );
+
+    if( window->State.JoystickLastPoll + window->State.JoystickPollRate <=
+        checkTime )
+    {
+#if !defined(_WIN32_WCE)
