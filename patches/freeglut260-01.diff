-#        endif
-#    endif
-
-#    ifdef JS_NEW
-
-    while ( 1 )
-    {
-        status = read ( joy->fd, &joy->js, sizeof(struct js_event) );
-
-        if ( status != sizeof( struct js_event ) )
-        {
-            if ( errno == EAGAIN )
-            {
-                /* Use the old values */
-                if ( buttons )
-                    *buttons = joy->tmp_buttons;
