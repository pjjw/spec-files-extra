+
+    if( pixelformat == 0 )
+        return GL_FALSE;
+
+    if( checkOnly )
+        return GL_TRUE;
+    return SetPixelFormat( window->Window.Device, pixelformat, ppfd );
+#endif /* defined(_WIN32_WCE) */
