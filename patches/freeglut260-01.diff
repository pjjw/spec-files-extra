-    case '=':
-    case '+': slices++;                   break;
-
-    case '-':
-    case '_': if( slices > -1 ) slices--; break;
-
-    case ',':
-    case '<': if( stacks > -1 ) stacks--; break;
-
-    case '.':
-    case '>': stacks++;                   break;
-
-    case '9': 
-    case '(': if( depth > -1 ) depth--;   break;
-
-    case '0': 
-    case ')': ++depth;                    break;
-
-    default:
-        break;
-    }
-
-    glutPostRedisplay();
-}
