-void FGAPIENTRY glutInit( int* pargc, char** argv )
-{
-    char* displayName = NULL;
-    char* geometry = NULL;
-    int i, j, argc = *pargc;
-
-    if( fgState.Initialised )
-        fgError( "illegal glutInit() reinitialization attempt" );
-
-    if (pargc && *pargc && argv && *argv && **argv)
-    {
