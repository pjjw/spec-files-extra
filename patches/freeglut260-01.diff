-static void fghClearCallBacks( SFG_Window *window )
-{
-    if( window )
-    {
-        int i;
-        for( i = 0; i < TOTAL_CALLBACKS; ++i )
-            window->CallBacks[ i ] = NULL;
-    }
-}
