-                   if (d)
-                       joy->os->cache_buttons |=  (1 << ( usage - 1 ));
-                   else
-                       joy->os->cache_buttons &= ~(1 << ( usage - 1 ));
-               }
-            }
-        }
-    }
-    if ( len < 0 && errno != EAGAIN )
-    {
-        perror( joy->os->fname );
