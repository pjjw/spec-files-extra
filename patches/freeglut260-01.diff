+    glutBitmapString ( GLUT_BITMAP_HELVETICA_12, (unsigned char*)line );
+  }
+
+  glMatrixMode ( GL_PROJECTION );
+  glPopMatrix ();
+  glMatrixMode ( GL_MODELVIEW );
+  glPopMatrix ();
+  glEnable ( GL_DEPTH_TEST );
+
+  printf ( "%6d Window %d Display Callback\n",
+            ++sequence_number, window ) ;
