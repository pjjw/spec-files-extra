+  if ( reshape_called )
+  {
+    sprintf ( line, "Reshape %d:  %d %d\n", reshape_seq, reshape_width, reshape_height );
+    glutBitmapString ( GLUT_BITMAP_HELVETICA_12, (unsigned char*)line );
+  }
+
+  if ( key_called )
